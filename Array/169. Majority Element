class Solution {
    public int majorityElement(int[] nums) {
        int count = 0, mar = 0;        
        for(int i =0; i < nums.length; i++) {
            if(count == 0) {
                mar = nums[i];
                count = 1;
            } else if(mar == nums[i]) {
                count++;  
            } else {
                count--;
            }
        }
        return mar;
    }
} //摩尔投票法
